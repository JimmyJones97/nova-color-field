{
  "name": "djstarcom/nova-color-field",
  "description": "A Laravel Nova Color Picker field.",
  "keywords": [
    "laravel",
    "nova",
    "color picker",
    "field"
  ],
  "authors": [
    {
      "name": "outl1ne",
      "homepage": "https://github.com/outl1ne"
    },
    {
      "name": "Stanislav Tsepeniuk",
      "homepage": "https://github.com/DJStarCOM"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.0",
    "laravel/nova": "^5.0",
    "ext-json": "*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3",
    "orchestra/testbench": "^9",
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "DJStarCOM\\NovaColorField\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "DJStarCOM\\NovaColorField\\ColorFieldServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://nova.laravel.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true
}
